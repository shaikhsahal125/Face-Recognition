<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="recognizeStyle" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RecognizeController">
   <children>
      <Text fill="#ebe4e4" layoutX="27.0" layoutY="113.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Image Path:">
         <font>
            <Font name="Arial Bold Italic" size="20.0" />
         </font>
      </Text>
      <TextField fx:id="inputPath" layoutX="147.0" layoutY="93.0" prefHeight="27.0" prefWidth="320.0" />
      <Button fx:id="browseBtn" layoutX="484.0" layoutY="89.0" mnemonicParsing="false" onAction="#browse" text="Browse">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="proceedBtn" layoutX="244.0" layoutY="156.0" mnemonicParsing="false" onAction="#proceed" text="Proceed">
         <font>
            <Font name="Times New Roman Bold Italic" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
